{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/app.js"
      },
      {
        "src": "/health",
        "dest": "/src/app.js"
      },
      {
        "src": "/styles.css",
        "dest": "/public/styles.css"
      },
      {
        "src": "/script.js",
        "dest": "/public/script.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }